{
  "name": "fe",
  "version": "0.0.0",
  "description": "fe",
  "license": "MIT",
  "engines": {
    "node": "18.x.x"
  },
  "scripts": {
    "ng": "ng",
    "analyze": "webpack-bundle-analyzer dist/server/stats.json",
    "start": "node dist/server/main.js",
    "build:stats": "ng build --configuration production && ng run fe:server:production --stats-json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build": "ng build --configuration production",
    "build:prod": "ng build --configuration production",
    "build:mobile": "ng build --configuration production",
    "build:canary": "ng build --configuration=canary",
    "build:staging": "ng build --configuration=staging",
    "deploy:run": "python3 ./deploy-run.py",
    "deploy:gae": "./deploy-gae-all.sh",
    "deploy:all:canary": "gcloud builds submit . --config=cloudbuild.yaml --project=dark-shade --substitutions=_VERSION=canary,_PROMOTE=--no-promote,_SITES=all",
    "deploy:superb:canary": "gcloud builds submit --config cloudbuild.yaml . --project=dark-shade --substitutions=_SITES=superb-ascent",
    "deploy:superb:prod": "gcloud builds submit --config cloudbuild.yaml . --project=dark-shade --substitutions=_SITES=superb-ascent,_PROMOTE=--promote,_VERSION=prod",
    "dev:ssr": "ng run fe:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build --configuration production && ng run fe:server:production",
    "build:ssr:prod": "ng build --configuration production && ng run fe:server:production",
    "build:ssr:canary": "ng build --configuration production && ng run fe:server:production",
    "build:ssr:production": "ng build --configuration production && ng run fe:server:production",
    "build:ssr:staging": "ng build --configuration=staging && ng run fe:server:staging",
    "prerender": "ng run fe:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.0.0",
    "@angular/cdk": "~16.0.0",
    "@angular/common": "^16.0.0",
    "@angular/compiler": "^16.0.0",
    "@angular/core": "^16.0.0",
    "@angular/fire": "^7.5.0",
    "@angular/forms": "^16.0.0",
    "@angular/material": "^16.0.0",
    "@angular/platform-browser": "^16.0.0",
    "@angular/platform-browser-dynamic": "^16.0.0",
    "@angular/platform-server": "^16.0.0",
    "@angular/router": "^16.0.0",
    "@nguniversal/express-engine": "^16.0.0",
    "@types/qrcode": "^1.5.0",
    "algoliasearch": "^4.17.0",
    "compression": "^1.7.4",
    "express": "^4.15.2",
    "firebase": "^9.16.0",
    "google-protobuf": "^3.11.0-rc.2",
    "ng-particles": "^2.5.1",
    "qrcode": "^1.4.4",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "tsparticles": "^1.22.1",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.0-next.7",
    "@angular/cli": "^16.0.0",
    "@angular/compiler-cli": "^16.0.0",
    "@nguniversal/builders": "^16.0.0",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.0",
    "@types/google-protobuf": "^3.15.6",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^14.15.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~5.0.2",
    "webpack-bundle-analyzer": "^4.8.0"
  }
}
